<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.androidanimation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AnimationMainActivity"
            android:theme="@style/ActivityTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".switchguideanim.FingerSwitchOnAnimActivity"
            android:theme="@style/ActivityTheme" />
        <activity
            android:name=".progressanim.ProgressActivity"
            android:theme="@style/ActivityTheme" />
        <activity
            android:name=".progressanim.ProgressSample2Activity"
            android:theme="@style/ActivityTheme" />
    </application>

</manifest>